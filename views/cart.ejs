<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../stylesheets/style.css">
</head>
<body>
  <div id="hey">

     
    <h1>This is your CART, Mr. <%= cart.name  %></h1>
    <div id="dibba">
      <div id="btn">  <a href="/prof"> Profile</a></div>
      <div id="btn"> <a href="/logout"> Logout</a></div>

    </div>
   
  </div>
    
  <div id="box">
    <% cart.cart.forEach(function(cart){ %>
         <div id="box1">
           <div id="box2">
           <img src="<%= cart.productimage%>" alt="">
         </div>
             <div id="box3">
               <h3><%= cart.productname %></h3>
             <div id="patti">
               <h4><%= cart.productprice %></h4>
               <form action="/deletecart/<%= cart.id %>" method="post">
               <input type="submit"  value="Remove">

               </form>

             </div>
             
            </div>
             
         </div>
    <% }) %>
 </div>

</body>
</html>